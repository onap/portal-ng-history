<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
        http://www.liquibase.org/xml/ns/dbchangelog-ext
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <changeSet  id="1"  author="dogukanakdogan" dbms="postgresql">
        <createTable  tableName="actions">
            <column  name="id"  type="text">
                <constraints primaryKey="true"/>
            </column>
            <column name="user_id" type="text"> </column>
            <column  name="action_created_at"  type="timestamp"> </column>
            <column  name="action"  type="jsonb"> </column>
        </createTable>
    </changeSet>
</databaseChangeLog>
